[Unit]
Description=Gunicorn server for DOMAIN

[Service]
Restart=on-failure
User=sdempsey
WorkingDirectory=/home/sdempsey/sites/DOMAIN
EnvironmentFile=/home/sdempsey/sites/DOMAIN/.EnvironmentFile

ExecStart=/home/sdempsey/sites/DOMAIN/virtualenv/bin/gunicorn --bind unix:/tmp/DOMAIN.socket superlists.wsgi:application

[Install]
WantedBy=multi-user.target